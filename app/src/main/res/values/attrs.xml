<resources>


    
    <declare-styleable name="SlidingTabLayout">
        <attr name="selectedColor" format="color" />
        <attr name="indicatorHeight" format="reference|dimension" />
        <attr name="dividerColor" format="color" />
        <attr name="android:textAppearance" />
        <attr name="contentInsetStart" />
    </declare-styleable>


    <declare-styleable name="CustomShapeImageView">
        <attr name="shape" format="integer">
            <enum name="circle" value="1" />
            <enum name="rectangle" value="2" />
            <enum name="svg" value="3" />
        </attr>
        <attr name="svg_raw_resource" format="reference" />
    </declare-styleable>

</resources>